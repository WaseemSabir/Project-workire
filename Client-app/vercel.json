{
    "version": 2,
    "public": true,
    "name": "workire-vercel",
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/api"
        }
    ],
    "functions": {
        "api/index.js": {
            "includeFiles": "dist/FrontEnd/browser/**"
        }
    },
    "installCommand": "npm install --legacy-peer-deps"
}